                    "enum": [
                        "text-embedding-ada-002",
                        "text-search-ada-doc-001",
                        "text-search-ada-query-001"
                    ],
                    "allow_manual_entry": true
                },
                "input": {
                    "type": [
                        "string"
                    ]
                }
            },
            "package": "promptflow-tools",
            "package_version": "1.4.0"
        },
